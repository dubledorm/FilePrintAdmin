<p id="notice"><%= notice %></p>

<h1>Printing Templates</h1>

<table>
  <thead>
    <tr>
      <th>Application type</th>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @printing_templates.each do |printing_template| %>
      <tr>
        <td><%= printing_template.application_type %></td>
        <td><%= printing_template.name %></td>
        <td><%= link_to 'Show', printing_template %></td>
        <td><%= link_to 'Edit', edit_printing_template_path(printing_template) %></td>
        <td><%= link_to 'Destroy', printing_template, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Printing Template', new_printing_template_path %>
